    "console": "integratedTerminal",
    "sourceMaps": true,
    "outFiles": ["${workspaceRoot}/build/**/*.js"],
    "protocol": "inspector",
    "windows": {
      "useWSL": false  
    },
  }, {
    "name": "Launch Awesome Demo",
    "type": "node",
    "request": "launch",
    "restart": false,
    "stopOnEntry": false,
    "args": ["lib/awesome.js"],
    "cwd": "${workspaceRoot}",
    "console": "integratedTerminal",
    "sourceMaps": true,
    "outFiles": ["${workspaceRoot}/build/**/*.js"],
    "protocol": "inspector",
    "windows": {
      "useWSL": false  
    },
  }]
